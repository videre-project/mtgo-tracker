{
  "name": "mtgo-tracker",
  "version": "0.1.0",
  "homepage": "https://mtgo.codyb.co",
  "description": "A lightweight open-source MTGO tracker. ",
  "repository": "https://github.com/CodyJasonBennett/mtgo-tracker.git",
  "author": "Cody Bennett <hi@codyb.co>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev src",
    "build": "tsc",
    "serve": "tsc && node build",
    "test": "jest --watchAll --verbose --testTimeout=10000",
    "test:ci": "jest .",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "build"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
